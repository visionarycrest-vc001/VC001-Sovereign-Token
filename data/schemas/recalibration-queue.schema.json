{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["id", "module", "reason", "scheduledFor"],
    "properties": {
      "id": { "type": "string" },
      "module": { "type": "string" },
      "reason": { "type": "string" },
      "scheduledFor": { "type": "string", "format": "date-time" }
    }
  }
}
